#! /usr/local/bin/vvp
:ivl_version "13.0 (devel)" "(s20221226-328-ga9ef00676)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x12b5980 .scope module, "pos_edge_tb" "pos_edge_tb" 2 2;
 .timescale -9 -9;
v0x12cbb00_0 .var "clk", 0 0;
v0x12cbbc0_0 .var "data", 0 0;
v0x12cbc90_0 .net "pos_edge", 0 0, L_0x12cbf30;  1 drivers
v0x12cbd90_0 .var "rstn", 0 0;
S_0x12b5b10 .scope module, "DUT" "pos_edge" 2 9, 3 1 0, S_0x12b5980;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "data";
    .port_info 2 /INPUT 1 "rstn";
    .port_info 3 /OUTPUT 1 "pos_edge";
L_0x12cbe60 .functor NOT 1, v0x12cb720_0, C4<0>, C4<0>, C4<0>;
L_0x12cbf30 .functor AND 1, L_0x12cbe60, v0x12cbbc0_0, C4<1>, C4<1>;
v0x12b5d10_0 .net *"_ivl_0", 0 0, L_0x12cbe60;  1 drivers
v0x12cb660_0 .net "clk", 0 0, v0x12cbb00_0;  1 drivers
v0x12cb720_0 .var "curr_sense", 0 0;
v0x12cb7f0_0 .net "data", 0 0, v0x12cbbc0_0;  1 drivers
v0x12cb8b0_0 .net "pos_edge", 0 0, L_0x12cbf30;  alias, 1 drivers
v0x12cb9c0_0 .net "rstn", 0 0, v0x12cbd90_0;  1 drivers
E_0x12b4650/0 .event negedge, v0x12cb9c0_0;
E_0x12b4650/1 .event posedge, v0x12cb660_0;
E_0x12b4650 .event/or E_0x12b4650/0, E_0x12b4650/1;
    .scope S_0x12b5b10;
T_0 ;
    %wait E_0x12b4650;
    %load/vec4 v0x12cb9c0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x12cb720_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x12cb7f0_0;
    %assign/vec4 v0x12cb720_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x12b5980;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12cbb00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12cbd90_0, 0, 1;
    %vpi_func 2 19 "$random" 32 {0 0 0};
    %pushi/vec4 2, 0, 32;
    %mod/s;
    %pad/s 1;
    %store/vec4 v0x12cbbc0_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x12cbb00_0;
    %inv;
    %store/vec4 v0x12cbb00_0, 0, 1;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
    .scope S_0x12b5980;
T_2 ;
    %pushi/vec4 2, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 4, 0;
    %load/vec4 v0x12cbd90_0;
    %inv;
    %store/vec4 v0x12cbd90_0, 0, 1;
    %vpi_call 2 28 "$display", "rstn after toggle: %b", v0x12cbd90_0 {0 0 0};
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %pushi/vec4 5, 0, 32;
T_2.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.3, 5;
    %jmp/1 T_2.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_func 2 32 "$random" 32 {0 0 0};
    %pushi/vec4 2, 0, 32;
    %mod/s;
    %pad/s 1;
    %store/vec4 v0x12cbbc0_0, 0, 1;
    %jmp T_2.2;
T_2.3 ;
    %pop/vec4 1;
    %delay 100, 0;
    %vpi_call 2 34 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x12b5980;
T_3 ;
    %vpi_call 2 38 "$display", "Simulation started" {0 0 0};
    %vpi_call 2 39 "$dumpfile", "./2_sim/pos_edge.vcd" {0 0 0};
    %vpi_call 2 40 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x12b5980 {0 0 0};
    %vpi_call 2 41 "$display", "Simulation finished" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "./1_tb/pos_edge_tb.v";
    "./0_design/pos_edge.v";
