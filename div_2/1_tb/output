#! /usr/local/bin/vvp
:ivl_version "13.0 (devel)" "(s20221226-328-ga9ef00676)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x11a6d70 .scope module, "div_2_tb" "div_2_tb" 2 2;
 .timescale -9 -12;
v0x11f12e0_0 .var "clk", 0 0;
v0x11f13b0_0 .net "clk_d", 0 0, v0x11f1100_0;  1 drivers
v0x11f1480_0 .var "rst_n", 0 0;
S_0x11a6f00 .scope module, "DUT" "div_2" 2 9, 3 1 0, S_0x11a6d70;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /OUTPUT 1 "clk_d";
v0x11a7170_0 .net "clk", 0 0, v0x11f12e0_0;  1 drivers
v0x11f1100_0 .var "clk_d", 0 0;
v0x11f11c0_0 .net "rst_n", 0 0, v0x11f1480_0;  1 drivers
E_0x11e0440 .event posedge, v0x11a7170_0;
    .scope S_0x11a6f00;
T_0 ;
    %wait E_0x11e0440;
    %load/vec4 v0x11f11c0_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11f1100_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x11f1100_0;
    %inv;
    %assign/vec4 v0x11f1100_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x11a6d70;
T_1 ;
    %vpi_call 2 14 "$display", "Simulation started" {0 0 0};
    %vpi_call 2 15 "$dumpfile", "./2_sim/div_2.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x11a6d70 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11f1480_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x11f12e0_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x11f1480_0, 0, 1;
    %vpi_call 2 21 "$display", "Simulation finished" {0 0 0};
    %delay 100000, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x11a6d70;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x11f12e0_0;
    %inv;
    %store/vec4 v0x11f12e0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "./1_tb/div_2_tb.v";
    "./0_design/div_2.v";
