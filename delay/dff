#! /usr/local/bin/vvp
:ivl_version "13.0 (devel)" "(s20221226-328-ga9ef00676)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 10;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x11b5390 .scope module, "d_ff_tb" "d_ff_tb" 2 2;
 .timescale -9 -10;
v0x11c87d0_0 .var/2u *"_ivl_0", 0 0; Local signal
v0x11c88d0_0 .var "clk", 0 0;
v0x11c8990_0 .var "d", 0 0;
v0x11c8a90_0 .net "q", 0 0, v0x11c8590_0;  1 drivers
v0x11c8b60_0 .var "reset", 0 0;
S_0x11b5520 .scope module, "dut" "d_ff" 2 13, 3 1 0, S_0x11b5390;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "d";
    .port_info 3 /OUTPUT 1 "q";
v0x11b5720_0 .net "clk", 0 0, v0x11c88d0_0;  1 drivers
v0x11c84d0_0 .net "d", 0 0, v0x11c8990_0;  1 drivers
v0x11c8590_0 .var "q", 0 0;
v0x11c8660_0 .net "reset", 0 0, v0x11c8b60_0;  1 drivers
E_0x117d430 .event posedge, v0x11b5720_0;
    .scope S_0x11b5520;
T_0 ;
    %wait E_0x117d430;
    %load/vec4 v0x11c8660_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x11c8590_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x11c84d0_0;
    %assign/vec4 v0x11c8590_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x11b5390;
T_1 ;
    %delay 50, 0;
    %load/vec4 v0x11c88d0_0;
    %inv;
    %store/vec4 v0x11c88d0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x11b5390;
T_2 ;
    %vpi_call 2 29 "$display", "Simulation started" {0 0 0};
    %vpi_call 2 30 "$dumpfile", "./2_sim/d_ff.vcd" {0 0 0};
    %vpi_call 2 31 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x11b5390 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11c88d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x11c8b60_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11c87d0_0, 0, 1;
    %pushi/vec4 50, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %load/vec4 v0x11c87d0_0;
    %store/vec4 v0x11c8b60_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x11c8990_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11c8990_0, 0, 1;
    %delay 50, 0;
    %vpi_call 2 41 "$display", "Simulation finished" {0 0 0};
    %delay 10000, 0;
    %vpi_call 2 43 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "/home/dchekuri/Desktop/WORK/RTL2GDS/delay/1_tb/d_ff_tb.v";
    "/home/dchekuri/Desktop/WORK/RTL2GDS/delay/0_design/d_ff.v";
